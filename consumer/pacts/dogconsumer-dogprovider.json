{
  "consumer": {
    "name": "DogConsumer"
  },
  "provider": {
    "name": "DogProvider"
  },
  "interactions": [
    {
      "description": "a request for dogs",
      "providerState": "I have one dog",
      "request": {
        "method": "GET",
        "path": "/dogs",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [
          {
            "dogs": [
              "bulldog",
              "schafer",
              "pug"
            ]
          }
        ]
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}